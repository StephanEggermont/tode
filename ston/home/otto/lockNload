TDScriptLeafNode{#name:'lockNload',#contents:'[ :topez :objIn :tokens :command | 
| opts args |
\"for help: ./lockNload -h\"
command
  getOptsMixedLongShort: {#(\'help\' $h #\'none\')}
  optionsAndArguments: [ :options :operands | 
    opts := options.
    args := operands ].
opts
  at: \'help\'
  ifAbsent: [ 
    Metacello new
      baseline: \'Example\';
      unregister.
    Metacello new
      baseline: \'Alternate\';
      unregister.
    Metacello new
      configuration: \'External\';
      unregister.
    Metacello new
      baseline: \'Sample\';
      unregister.
    Metacello new
      baseline: \'Alternate\';
      repository: \'filetree:///opt/git/otto/alternate/repository\';
      lock.
    Metacello new
      configuration: \'External\';
      version: \'1.0.0\';
      repository: \'server:///opt/git/otto/extDir\';
      lock.
    Metacello new
      baseline: \'Sample\';
      repository: \'filetree:///opt/git/otto/sample/repository\';
      lock.
    Metacello new
      baseline: \'Example\';
      repository: \'filetree:///opt/git/otto/example/repository\';
      get.
    Metacello registry
      baseline: \'Example\';
      onLock: [ :ex | ex honor ];
      onConflict: [ :ex :existing | 
            existing locked
              ifTrue: [ ex disallow ] ];
      load ]
  ifPresent: [ :ignored | 
    TDManPage
      viewManPage:
        \'NAME
  lockNload - lockNload sript utility template
SYNOPSIS
  lockNload [-h|--help] 

DESCRIPTION
  lock the External and Sample projects, then load the example proejct using the 
  following script:

    ./load --baseline=Example --repo=@exampleDir --load=Tests
EXAMPLES
  ./lockNload --help
  ./lockNload -h

  ./lockNload
\'
      topez: topez ] ]',#creationTime:DateAndTime['2013-11-23T13:35:19.71847891807556-08:00'],#modificationTime:DateAndTime['2013-11-23T14:11:10.95439505577087-08:00']}