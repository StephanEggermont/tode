TDSmalltalkLeafNode{#name:'xx',#contents:'| nsc indexSpec query inst profMonitor |
nsc := (IXFamilyTree createFamily: 6) population.
indexSpec := GsIndexSpec new
  equalityIndex: \'each.sons.*.numberOfChildren\' lastElementClass: Integer;
  yourself.
[ 
indexSpec createIndexesOn: nsc.
query := (GsQuery fromString: \'2 < each.sons.*.numberOfChildren <= 4\')
  on: nsc;
  optimize;
  yourself.
inst := ProfMonitor new.
inst monitorBlock: [ 100 timesRepeat: [ query asIdentityBag ] ].
profMonitor := TDProfMonitor new
  profMonResults: inst results;
  yourself.
inst removeResults.
profMonitor
  editUsing:
    ((TDEditorSpec topez: nil topez editorAspect: #\'profMonResults\')
      windowocation: #\'debugger\';
      yourself).
^ profMonitor ]
  ensure: [ indexSpec removeIndexesFrom: nsc ]',#creationTime:nil,#modificationTime:nil}