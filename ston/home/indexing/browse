TDScriptLeafNode{#name:'browse',#contents:'[ :topez :objIn :tokens :windowId | 
| findTool defs indexSpecDefs indexSpecClassNames |
findTool := topez toolInstanceFor: \'find\'.
defs := findTool findProjectDefinitions: \'Indexing\'.
indexSpecDefs := findTool findClassHierarchy: \'AbstractIndexSpecification\'.
\tindexSpecDefs := indexSpecDefs reject: [:def | def className == #Object].
indexSpecClassNames := indexSpecDefs collect: [ :def | def className ].
defs := defs reject: [ :def | indexSpecClassNames includes: def className ].
defs addAll: indexSpecDefs.
findTool browseClasses: defs.
defs ]'}